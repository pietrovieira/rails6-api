<p id="notice"><%= notice %></p>

<h1>Order Shippings</h1>

<table>
  <thead>
    <tr>
      <th>Order</th>
      <th>Address</th>
      <th>Num</th>
      <th>Neighborhood</th>
      <th>City</th>
      <th>Uf</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @order_shippings.each do |order_shipping| %>
      <tr>
        <td><%= order_shipping.order_id %></td>
        <td><%= order_shipping.address %></td>
        <td><%= order_shipping.num %></td>
        <td><%= order_shipping.neighborhood %></td>
        <td><%= order_shipping.city %></td>
        <td><%= order_shipping.uf %></td>
        <td><%= link_to 'Show', order_shipping %></td>
        <td><%= link_to 'Edit', edit_order_shipping_path(order_shipping) %></td>
        <td><%= link_to 'Destroy', order_shipping, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Order Shipping', new_order_shipping_path %>
